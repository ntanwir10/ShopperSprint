{
  "name": "pricepulse",
  "version": "1.0.0",
  "description": "PricePulse - A comprehensive full-stack web application for tracking and comparing product prices across multiple online sources",
  "main": "index.js",
  "scripts": {
    "setup": "./scripts/automate.sh setup",
    "quick": "./scripts/automate.sh quick",
    "dev": "./scripts/automate.sh dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "./scripts/automate.sh build",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "cd backend && npm start",
    "test": "./scripts/automate.sh test",
    "test:coverage": "cd backend && npm run test:coverage",
    "health": "./scripts/automate.sh health",
    "db:start": "docker-compose up -d postgres redis",
    "db:stop": "docker-compose down",
    "db:setup": "npm run db:start && sleep 5 && cd backend && npm run db:push && npm run db:seed",
    "db:reset": "./scripts/reset-database.sh",
    "deploy": "./scripts/automate.sh deploy prod",
    "deploy:staging": "./scripts/automate.sh deploy staging",
    "deploy:prod": "./scripts/automate.sh deploy prod",
    "deploy:railway": "./scripts/deploy-railway.sh",
    "deploy:railway:single": "./scripts/deploy-railway.sh single",
    "deploy:railway:separate": "./scripts/deploy-railway.sh separate",
    "deploy:railway:backend": "./scripts/deploy-railway.sh backend",
    "deploy:railway:frontend": "./scripts/deploy-railway.sh frontend",
    "automate": "./scripts/automate.sh"
  },
  "keywords": [
    "pricepulse",
    "e-commerce",
    "price-comparison",
    "web-scraping",
    "react",
    "nodejs",
    "typescript",
    "postgresql",
    "redis"
  ],
  "author": "ntanwir",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@vercel/node": "^3.2.29",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0"
  }
}
